<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="users">

	<select id="selectUser" resultType="userVo"	parameterType="String">
		SELECT *
		FROM users
		WHERE userid = #{userid}
	</select>

</mapper>